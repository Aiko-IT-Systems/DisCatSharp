	{{/_providerName}}
	{{#_providerUrl}}
	<meta name="provider_url" content="{{_providerUrl}}">
	{{/_providerUrl}}
	{{#keywords}}
	<meta name="keywords" content="{{keywords}}">
	{{/keywords}}
	<meta name="robots" content="index, follow, max-image-preview:large">
	<meta name="googlebot" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1">
	<meta name="bingbot" content="index, follow">
	<meta name="rating" content="general">
	<link rel="sitemap" type="application/xml" title="Sitemap" href="{{_rel}}sitemap.xml">

	<!-- General OpenGraph -->
	<meta property="og:title"
		content="{{#title}}{{title}}{{/title}}{{^title}}{{>partials/title}}{{/title}} {{#_appTitle}}| {{_appTitle}} {{/_appTitle}}">
	<meta property="og:description" content="{{description}}">
	{{#_ogType}}
	<meta property="og:type" content="{{_ogType}}">
	<meta property="article:section" content="Technology">
	<meta property="article:tag" content="bot development">
	{{/_ogType}}
	{{#_ogImage}}
	<meta property="og:image" content="{{_ogImage}}">
	<meta property="og:image:alt" content="{{#title}}{{title}} {{/title}}Logo">
	<meta property="og:image:type" content="image/png">
	{{/_ogImage}}
	{{#_appName}}
	<meta property="og:site_name" content="{{_appName}}">
	{{/_appName}}
	{{#_lang}}
	<meta property="og:locale" content="{{_lang}}">
	{{/_lang}}
	{{#_pDomainVerification}}
	<meta name="p:domain_verify" content="{{_pDomainVerification}}" />
	{{/_pDomainVerification}}
	{{#_yandexVerification}}
	<meta name="yandex-verification" content="{{_yandexVerification}}" />
	{{/_yandexVerification}}

	<!-- PWA -->
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
	<meta name="apple-mobile-web-app-title" content="{{#_appTitle}}{{_appTitle}}{{/_appTitle}}">
	<meta name="application-name" content="{{#_appTitle}}{{_appTitle}}{{/_appTitle}}">
	<meta name="mobile-web-app-capable" content="yes">
	<meta name="theme-color" content="#fc0fc0">

	<!-- Facebook OpenGraph -->
	{{#_fbPageId}}
	<meta property="fb:pages" content="{{_fbPageId}}">
	{{/_fbPageId}}
	{{#_fbAppId}}
	<meta property="fb:app_id" content="{{_fbAppId}}">
	{{/_fbAppId}}

	<!-- Twitter OpenGraph -->
	<meta name="twitter:card" content="summary_large_image">
	{{#_twitterSite}}
	<meta name="twitter:site" content="{{_twitterSite}}">{{/_twitterSite}}
	<meta name="twitter:creator" content="@aiko_it_systems">
	<meta name="twitter:title"
		content="{{#title}}{{title}}{{/title}}{{^title}}{{>partials/title}}{{/title}} {{#_appTitle}}| {{_appTitle}} {{/_appTitle}}">
	<meta name="twitter:description" content="{{description}}">
	{{#_twitterImage}}
	<meta name="twitter:image" content="{{_twitterImage}}">
	<meta name="twitter:image:alt" content="{{#title}}{{title}} {{/title}}Logo">
	{{/_twitterImage}}

	<!-- Docfx Meta -->
	<meta name="docfx:tocrel" content="{{_tocRel}}">
	{{#_noindex}}
	<meta name="searchOption" content="noindex">
	{{/_noindex}}
	{{#_enableSearch}}
	<meta name="docfx:rel" content="{{_rel}}">
	{{/_enableSearch}}
	{{#_disableNewTab}}
	<meta name="docfx:disablenewtab" content="true">
	{{/_disableNewTab}}
	{{#_disableTocFilter}}
	<meta name="docfx:disabletocfilter" content="true">
	{{/_disableTocFilter}}
	{{#docurl}}
	{{#_urlNormalizationActive}}
	<meta name="docfx:urlnormalizationactive" content="true">
	{{/_urlNormalizationActive}}
	<meta name="docfx:docurl" content="{{docurl}}">
	{{/docurl}}
	<meta name="docfx:navrel" content="{{_navRel}}">
	<meta name="loc:inThisArticle" content="{{__global.inThisArticle}}">
	<meta name="loc:searchResultsCount" content="{{__global.searchResultsCount}}">
	<meta name="loc:searchNoResults" content="{{__global.searchNoResults}}">
	<meta name="loc:tocFilter" content="{{__global.tocFilter}}">
	<meta name="loc:nextArticle" content="{{__global.nextArticle}}">
	<meta name="loc:prevArticle" content="{{__global.prevArticle}}">
	{{^_disableThemeSwitcher}}
	<meta name="loc:themeLight" content="{{__global.themeLight}}">
	<meta name="loc:themeDark" content="{{__global.themeDark}}">
	<meta name="loc:themeAuto" content="{{__global.themeAuto}}">
	<meta name="loc:changeTheme" content="{{__global.changeTheme}}">
	{{/_disableThemeSwitcher}}
	<meta name="loc:copy" content="{{__global.copy}}">
	<meta name="loc:downloadPdf" content="{{__global.downloadPdf}}">

	<!-- Manifests -->
	<link rel="manifest" href="{{_rel}}app.webmanifest">
	<link rel="manifest" href="{{_rel}}manifest.json">

	<!-- Icons -->
