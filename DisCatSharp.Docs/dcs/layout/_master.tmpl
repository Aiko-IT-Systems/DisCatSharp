	<link rel="apple-touch-icon" sizes="114x114" href="{{_rel}}apple-icon-114x114.png">
	<link rel="apple-touch-icon" sizes="120x120" href="{{_rel}}apple-icon-120x120.png">
	<link rel="apple-touch-icon" sizes="144x144" href="{{_rel}}apple-icon-144x144.png">
	<link rel="apple-touch-icon" sizes="152x152" href="{{_rel}}apple-icon-152x152.png">
	<link rel="apple-touch-icon" sizes="180x180" href="{{_rel}}apple-icon-180x180.png">
	<link rel="shortcut icon" type="image/png" sizes="16x16" href="{{_rel}}favicon-16x16.png">
	<link rel="shortcut icon" type="image/png" sizes="32x32" href="{{_rel}}favicon-32x32.png">
	<link rel="shortcut icon" type="image/png" sizes="96x96" href="{{_rel}}favicon-96x96.png">
	<link rel="shortcut icon" type="image/png" sizes="36x36" href="{{_rel}}android-icon-36x36.png">
	<link rel="shortcut icon" type="image/png" sizes="48x48" href="{{_rel}}android-icon-48x48.png">
	<link rel="shortcut icon" type="image/png" sizes="96x96" href="{{_rel}}android-icon-96x96.png">
	<link rel="shortcut icon" type="image/png" sizes="144x144" href="{{_rel}}android-icon-144x144.png">
	<link rel="shortcut icon" type="image/png" sizes="192x192" href="{{_rel}}android-icon-192x192.png">
	<link rel="mask-icon" href="{{_rel}}logo.svg" color="black">
	<link rel="icon" sizes="192x192" href="{{_rel}}apple-icon.png">
	<meta name="msapplication-square70x70logo" content="{{_rel}}ms-icon-70x70.png">
	<meta name="msapplication-square144x144logo" content="{{_rel}}ms-icon-144x144.png">
	<meta name="msapplication-square150x150logo" content="{{_rel}}ms-icon-150x150.png">
	<meta name="msapplication-square310x310logo" content="{{_rel}}ms-icon-310x310.png">
	<meta name="msapplication-TileColor" content="#fc0fc0">
	<meta name="msapplication-TileImage" content="{{_rel}}ms-icon-144x144.png">
	<link rel="icon" href="{{_rel}}{{{_appFaviconPath}}}{{^_appFaviconPath}}favicon.ico{{/_appFaviconPath}}"
		type="image/x-icon">
	<link rel="shortcut icon" href="{{_rel}}{{{_appFaviconPath}}}{{^_appFaviconPath}}favicon.ico{{/_appFaviconPath}}"
		type="image/x-icon">

	<!-- Tweaks -->
	<link rel="preload" as="image" href="{{_rel}}logo.svg" crossorigin="anonymous">
	<link rel="preload" href="{{_rel}}public/docfx.min.css" as="style" crossorigin="anonymous">
	<link rel="preload" href="{{_rel}}public/main.css" as="style" crossorigin="anonymous">
	<link rel="preload" href="{{_rel}}public/main.js" as="script" crossorigin="anonymous">
	<link rel="preload" href="{{_rel}}public/docfx.min.js" as="script" crossorigin="anonymous">
	<link rel="preconnect" href="https://fonts.googleapis.com" crossorigin="anonymous">
	<link rel="dns-prefetch" href="https://fonts.googleapis.com" crossorigin="anonymous">
	{{^_disableFeatherlight}}
