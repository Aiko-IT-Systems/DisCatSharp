	"build": {
		"xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"],
		"xref": [
			"ext-xrefmap.yml",
			"https://learn.microsoft.com/en-us/dotnet/.xrefmap.json"
		],
		"content": [
			{
				"files": [
					"api/**/**.yml",
					"api/**/**.md",
					"api/**.yml",
					"api/**.md"
				],
				"exclude": ["libdev/**", "**/node_modules/**"]
			},
			{
				"files": [
					"**.md",
					"toc.yml",
					"faq/**.yml",
					"faq/**.md",
					"articles/**.yml",
					"articles/**.md",
					"changelogs/**.yml",
					"changelogs/**.md",
					"natives/**.yml",
					"natives/**.md",
					"vs/**.yml",
					"vs/**.md"
				],
				"exclude": [
					"**/bin/**",
					"**/obj/**",
					"_site/**",
					"dcs/**",
					"**/node_modules/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**",
					"natives/**.zip",
					"natives/**.jar",
					"./.htaccess",
					"snippets/**"
				],
				"exclude": [
					"**/bin/**",
					"**/obj/**",
					"_site/**",
					"images/_**",
					"**/node_modules/**"
				]
			}
		],
		"overwrite": [],
		"dest": "_site",
		"globalMetadata": {
			"pdf": false,
			"pdfTocPage": false,
			"pdfFileName": "dcs-docs.pdf",
			"pdfCoverPage": "pdf/cover.html",
			"pdfFooterTemplate": "pdf/footer.html",
			"_appTitle": "DisCatSharp Docs",
			"_appName": "DisCatSharp",
			"_appFooter": "Made with ❤️ by AITSYS",
			"_enableSearch": true,
			"_disableNextArticle": false,
			"_disableSideFilter": false,
			"_disableTocFilter": false,
			"_disableNewTab": false,
			"_disableContribution": false,
			"_disableBreadcrumb": false,
			"_disableThemeSwitcher": true,
			"_gitUrlPattern": "git",
			"_gitContribute": {
				"repo": "https://github.com/Aiko-IT-Systems/DisCatSharp",
				"branch": "main"
			},
			"_googleTagId": "G-8JHG7YVNKK",
			"_lang": "en-US",
			"_ogType": "website",
			"_ogImage": "/dcs-docs-embeds-logo.png",
			"_fbPageId": "112155734832129",
			"_twitterSite": "@DisCatSharp",
			"_twitterImage": "/dcs-docs-embeds-logo.png",
			"_layout": "conceptual",
			"_urlNormalizationActive": true,
			"_providerName": "AITSYS DocFx",
			"_providerUrl": "https://github.com/Aiko-IT-Systems/docfx",
			"description": "Your library to write discord bots in C# with focus on always providing access to the latest discord features."
		},
		"disableGitFeatures": false,
		"exportRawModel": false,
		"exportViewModel": false,
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"fileMetadata": {
			"_ogType": {
				"articles/**/*.md": "article",
				"changelogs/**/*.md": "article",
				"natives/**/*.md": "article",
				"vs/**/*.md": "article"
			}
		},
		"template": ["default", "modern", "dcs"],
		"postProcessors": ["ExtractSearchIndex"],
		"noLangKeyword": false,
		"keepFileLink": true,
		"sitemap": {
			"baseUrl": "https://docs.dcs.aitsys.dev/",
			"changefreq": "daily",
			"priority": 1.0
		},
		"markdownEngineProperties": {
			"markdigExtensions": ["attributes"]
		}
	}
